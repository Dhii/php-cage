{
  "name": "dhii/php-cage",
  "description": "Encapsulates PHP code into a namespace",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Miguel Muscat",
      "email": "miguelmuscat93@gmail.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "nikic/php-parser": "^4.9@dev",
    "symfony/console": "^5.2.0",
    "symfony/finder": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0",
    "vimeo/psalm": "^3.11.7",
    "slevomat/coding-standard": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\PhpCage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhii\\PhpCage\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-initial": "0.1.x-dev"
    }
  }
}
